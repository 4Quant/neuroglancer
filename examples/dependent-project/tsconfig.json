{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "preserveConstEnums": true,
    "strictNullChecks": false,
    "sourceMap": true,
    "module": "commonjs",
    "target": "ES2015",
    "newLine": "LF",
    "baseUrl": ".",
    "paths": {
      "*": ["*"],
      "neuroglancer/*": ["third_party/neuroglancer/*"],
      "my-neuroglancer-project/*": ["src/my-neuroglancer-project/*"]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
